{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "POST my beer",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "identification": {
            "description": "unique id",
            "type": [
                "string"
            ],
            "pattern": "string"
        },
        "beer_date": {
            "description": "date",
            "type": "string"
        },
        "beer_name": {
            "description": "beer_name",
            "type": "string"
        }
    },
    "required": [ "identification", "beer_date", "beer_name" ]
}
